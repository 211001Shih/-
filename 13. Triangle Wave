#include<bits/stdc++.h>
using namespace std;
int main(){
	int a,b,c;
	cin>>a;
	for(int i=0;i<a;i++){
		cin>>b>>c;
		for(int o=0;o<c;o++){
			for(int j=1;j<=b;j++){
				for(int k=1;k<=j;k++)cout<<j;
				cout<< endl;
			}
			for(int j=b-1;j>=1;j--){
				for(int k=1;k<=j;k++)cout<<j;
				cout<<endl;	
			}
		if(c>1){
			cout<<endl;		
			}
		}		
	}
}
